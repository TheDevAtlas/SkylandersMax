#define ${TARGET_TYPE}

1 VERSIONINFO
FILEVERSION ${PROJECT_VERSION_MAJOR_},${PROJECT_VERSION_MINOR_},${PROJECT_VERSION_PATCH_},${PROJECT_VERSION_TWEAK_}
PRODUCTVERSION ${PROJECT_VERSION_MAJOR_},${PROJECT_VERSION_MINOR_},${PROJECT_VERSION_PATCH_},${PROJECT_VERSION_TWEAK_}
FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
FILEFLAGS 0x1L
#else
FILEFLAGS 0x0L
#endif
FILEOS 0x40004L
#ifdef SHARED_LIBRARY
FILETYPE 0x2L
#else
FILETYPE 0x1L
#endif
FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000904b0"
        BEGIN
            VALUE "CompanyName", "${TARGET_AUTHOR}"
            VALUE "FileDescription", "${TARGET_DESC}"
            VALUE "FileVersion", "${PROJECT_VERSION_}"
            VALUE "InternalName", "${TARGET_NAME}"
            VALUE "LegalCopyright", "${TARGET_COPYRIGHT}${TARGET_AUTHOR}"
            VALUE "OriginalFilename", "${TARGET_PREFIX}${TARGET_NAME}${TARGET_SUFFIX}"
            VALUE "ProductName", "${TARGET_PRODUCT_NAME}"
            VALUE "ProductVersion", "${PROJECT_VERSION_}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x9, 1200
    END
END
